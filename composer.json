{
    "name": "ArthurGuy/BBMS",
    "description": "Build Brighton Member System",
    "authors": [
        {
            "name": "Arthur Guy",
            "email": "arthur@arthurguy.co.uk"
        }
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "laravel/framework": "~5.1.0",
        "illuminate/html": "~5.0",
        "gocardless/gocardless": "0.4.2",
        "nesbot/Carbon": "~1.19",
        "rap2hpoutre/laravel-log-viewer": "~0.2",
        "nuovo/spreadsheet-reader": "~0.5",
        "intervention/image": "2.*",
        "paypal/merchant-sdk-php": "3.8.*",
        "laracasts/presenter": "~0.2",
        "stripe/stripe-php": "1.*",
        "michelf/php-markdown": "~1.4",
        "guzzlehttp/guzzle": "~5.0",
        "jenssegers/rollbar": "~1.4",
        "maknz/slack": "~1.5",
        "sybio/gif-creator": "~1.0",
        "filp/whoops": "~1.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "pusher/pusher-php-server": "^2.2",
        "arthurguy/notifications": "~1.0",
        "doctrine/dbal": "^2.5",
        "itsgoingd/clockwork": "~1.8",
        "zircote/swagger-php": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "phpspec/phpspec": "~2.1",
        "mockery/mockery": "~0.9",
        "codeception/codeception": "~2.1@beta",
        "codeception/mockery-module": "~0.2",
        "fzaninotto/faker": "~1.4"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "BB\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-install-cmd": [
        ],
        "pre-update-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "php artisan optimize"
        ],
        "post-create-project-cmd": [
            "php -r \"copy('.env.example', '.env');\"",
            "php artisan key:generate"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
