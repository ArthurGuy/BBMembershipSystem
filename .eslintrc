{
    "env": {
        "browser": true
    },
    "rules": {
        "no-cond-assign": 2,
        "no-constant-condition": 2,
        "no-debugger": 1,
        "no-dupe-keys": 2,
        "no-dupe-args": 2,
        "no-duplicate-case": 2,
        "no-empty": 2,
        "no-ex-assign": 2,
        "no-extra-semi": 2,
        "no-inner-declarations": 2,
        "no-invalid-regexp": 2,
        "no-irregular-whitespace": 2,
        "no-obj-calls": 2,
        "no-regex-spaces": 2,
        "no-unreachable": 2,
        "use-isnan": 2,
        "valid-typeof": 2,
        "no-unexpected-multiline": 2,
        "curly": 2,
        "dot-notation": [2, {"allowKeywords": false}],
        "eqeqeq": [2, "smart"],
        "no-caller": 2,
        "no-extend-native": 2,
        "no-extra-bind": 2,
        "no-floating-decimal": 2,
        "no-implied-eval": 2,
        "no-iterator": 2,
        "no-lone-blocks": 2,
        "no-loop-func": 2,
        "no-native-reassign": 2,
        "no-new-func": 2,
        "no-new-wrappers": 2,
        "no-proto": 2,
        "no-return-assign": 2,
        "no-script-url": 2,
        "no-self-compare": 2,
        "no-sequences": 2,
        "no-throw-literal": 2,
        "no-unused-expressions": 2,
        "no-useless-call": 2,
        "no-useless-concat": 2,
        "no-with": 2,
        "no-shadow-restricted-names": 2,
        "no-undef-init": 2,
        "no-unused-vars": 2,
        "no-use-before-define": 2,
        "indent": 2,
        "new-cap": 2,
        "new-parens": 2,
        "no-lonely-if": 2,
        "no-mixed-spaces-and-tabs": [2, "smart-tabs"],
        "no-spaced-func": 2,
        "no-trailing-spaces": 2,
        "semi": 2
    }
}